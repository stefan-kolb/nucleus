ion":["keep-alive"]},"body":{"encoding":"CP850","string":"{\"name\": \"nucleusappallproperties2/nucleus\", \"stack\": {\"name\": \"pinky\"}, \"dep_id\": \"depm5y3dy4k\", \"branch\": \"ssh://nucleusappallproperties2@cloudcontrolled.com/repository.git\", \"users\": [], \"static_files\": \"(removed)\", \"billing_account\": null, \"version\": \"b6b75520eb2ea08f08c318209b92cab3ba04b1de\", \"state\": \"deployed\", \"date_created\": \"2015-10-23T09:08:16\", \"date_modified\": \"2015-10-23T09:13:14\", \"is_default\": false, \"default_subdomain\": \"nucleus-nucleusappallproperties2.cloudcontrolled.com\", \"min_boxes\": 1, \"max_boxes\": 1, \"billed_boxes\": {\"costs\": 0.0, \"boxes\": 0.0, \"until\": 1445591620, \"free_boxes\": 0.0}, \"billed_addons\": []}"},"http_version":null},"recorded_at":"Fri, 23 Oct 2015 09:13:55 GMT"},{"request":{"method":"get","uri":"http://nucleus-nucleusappallproperties2.cloudcontrolled.com/","body":{"encoding":"US-ASCII","string":""},"headers":{"User-Agent":["excon/0.45.4"]}},"response":{"status":{"code":200,"message":null},"headers":{"Content-Type":["text/html; charset=utf-8"],"X-Powered-By":["Express"],"ETag":["\"-1541606901\""],"x-host":["nucleus-nucleusappallproperties2.cloudcontrolled.com"],"x-url":["/"],"Content-Length":["3290"],"Accept-Ranges":["bytes"],"Date":["Fri, 23 Oct 2015 09:13:40 GMT"],"X-Varnish":["696935350"],"Age":["0"],"Via":["1.1 varnish"],"Connection":["keep-alive"],"X-varnish-cache":["MISS"]},"body":{"encoding":"UTF-8","string":"<!DOCTYPE html>\n<html>\n  <head>\n    <title>Word Finder</title>\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <script src=\"/public/jquery-1.8.0.js\"></script>\n    <link rel=\"stylesheet\" href=\"/public/bootstrap.css\" />\n    <script src=\"/public/bootstrap.js\"></script>\n    <link rel=\"stylesheet\" href=\"/public/app.css\" />\n  </head>\n  <body>\n    <div class=\"navbar navbar-inverse navbar-fixed-top\">\n      <div class=\"container\">\n        <div class=\"navbar-header\">\n          <button type=\"button\" class=\"navbar-toggle\" data-toggle=\"collapse\" data-target=\".navbar-collapse\">\n            <span class=\"icon-bar\"></span>\n            <span class=\"icon-bar\"></span>\n            <span class=\"icon-bar\"></span>\n          </button>\n          <a class=\"navbar-brand\" href=\"/\">Word Finder</a>\n        </div>\n        <div class=\"collapse navbar-collapse\">\n          <ul class=\"nav navbar-nav\">\n          </ul>\n        </div><!--/.nav-collapse -->\n      </div>\n    </div>\n\n    <div class=\"container\">\n      <div id=\"dashboard\">\n        <div class=\"row\">\n          <h3>Word Finder</h3>\n\n          <p>\n          I took every english word (over 200k words) and built a little app that will help you find words that contain specific characters.\n          </p>\n          <hr/>\n        </div>\n\n\n        <div class=\"row\">\n            <p>Enter a word, use _ and ? as needed (see instructions below)</p>\n          <form method=\"post\" action=\"search\">\n            \n              <input type=\"text\"\n                     class=\"col-md-12\"\n                     name=\"pattern\"\n                     placeholder=\"enter pattern and press\" />\n           \n          </form>\n        </div>\n        <hr/>\n\n\n\n\n          <div class=\"row\">\n            <h3>The underscore</h3>\n\n            <p>\n            Type a word into the text box with the following pattern:\n            </p>\n\n            <pre>\nhe__o\n            </pre>\n\n            <p>\n            And you'll get words such as:\n            </p>\n\n            <pre>\nhello\nhelio\n            </pre>\n          </div>\n\n          <div class=\"row\">\n            <h3>The question mark</h3>\n\n            <p>\n            This character is great for games like What's the Phrase (a knock off of Wheel of Fortune)\n            </p>\n\n            <p>\n            Type a word into the text box with the following pattern:\n            </p>\n\n            <pre>\nst???\n            </pre>\n\n            <p>\n            and you'll get words such as:\n            </p>\n\n            <pre>\nstack\nstade\nstaff\nstage\nstagy\n            </pre>\n\n            <p>\n            but you wont get words like\n            </p>\n\n            <pre>\nstart\n            </pre>\n\n            <p>\n            because the `t` would already be visible (in What's the Phrase), and you would have typed:\n            </p>\n\n            <pre>\nst??t\n            </pre>\n\n          </div>\n          <div class=\"row\">\n            <h3>I know regex</h3>\n\n            <p>\n                The behaviors above will be overriden if you prefix and suffix your phrase with <code>/</code>, example:\n            </p>\n\n            <pre>/boo/</pre>\n\n            <p>\n                Will give you all words that contain <code>boo</code>.\n            </p>\n          </div>\n        </div>\n      </div>\n    </body>\n</html>\n\n"},"http_version":null},"recorded_at":"Fri, 23 Oct 2015 09:13:55 GMT"}],"recorded_with":"VCR 2.9.3"}